def Summation(offset,*args):
    result = 0
    for arg in args:
        result = 0
        result += arg
        
    return result
    
    
def main():
    print(Summation(15,30,20,40))
    print(Summation(1,2,3))
    
    
    myNums = [5,10,15,20]
    print(Summation(*myNums))
    
if __name__ == "__main__":
    main()
